pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
            stage('Build') {
            steps {
                echo 'Build'
            }
            }
            stage('Deployment') {
            steps {
                echo 'project Deployment'
            }
        }
        stage('Testing') {
            steps {
                echo 'Testing'
            }
    }
    stage('Release') {
            steps {
                echo 'Release'
            }
}

}

}










